/* IF-PMIC configurations */
&i2c5 {
	sm5705q@25 {
		compatible = "muic-universal";
		reg = <0x25>;
		interrupt-parent = <&eintc>;
		interrupts = <12 IRQ_TYPE_EDGE_FALLING>;
		pinctrl-names = "default";
		pinctrl-0 = <&if_pmic_default &if_pmic_irq>;

		muic-universal,chip_name = "sm,sm5705";
		muic-universal,irq-gpio = <&pio 12 GPIO_ACTIVE_LOW>;
		muic,max-afc-support-volt = <4>;
		muic,max-afc-support-cur = <8>;
		status = "okay";
	};

	muic {
		status = "okay";
		muic,support-list =
					"+OTG:GND",
					"-MHL:1K",
					"-VZW Accessory:28.7K",
					"-VZW Incompatible:34K",
					"-Smartdock:40.2K",
					"-HMT:49.9K",
					"-Audiodock:64.9K",
					"-USB LANHUB:80.07K",
					"-Charging Cable:102K",
					"-Universal Multimedia dock:121K",
					"+Jig USB Off:255K",
					"+Jig USB On:301K",
					"+Deskdock:365K",
					"+TYPE2 Charger:442K",
					"+Jig UART Off:523K",
					"+Jig UART On:619K",
					"+TA:OPEN",
					"+USB:OPEN",
					"+CDP:OPEN",
					"+Undefined Charging:XXX";
	};

	sm5705@49 {
		compatible = "sm,sm5705";
		reg = <0x49>;
		interrupt-parent = <&eintc>;
		sm5705,irq-gpio = <&pio 8 0>;
		pinctrl-names = "mrstb_deault", "chg_int_default";
		pinctrl-0 = <&charger_mrstb_default &charger_int_default>;
		sm5705,mrstb-gpio = <&pio 13 0>;
	};
};
	

