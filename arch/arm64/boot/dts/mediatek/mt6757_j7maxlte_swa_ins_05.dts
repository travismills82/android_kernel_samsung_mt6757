&smartglow {
	pinctrl-names = "default", "s_led_en";
	pinctrl-0 = <&smartglow_default>;
	pinctrl-1 = <&smartglow_led_en>;

	et6312b,iMax = <0x0>;
	et6312b,clk_mode = <0x0>;
	
	et6312b,color_range = < 8
		/* Index		Rmin	Rmax	Gmin	Gmax	Bmin	Bmax	Comment */
		 1 /* 001 */  	140		255		140		255		140		255		/* Only Blue */
		 2 /* 010 */  	140		255		140		255		140		255		/* Only Green */
		 4 /* 100 */  	140		255		140		255		140		255		/* Only Red */
		 3 /* 011 */  	140		255		140		255		140		255		/* Green & Blue */
		 5 /* 101 */  	140		255		140		255		140		255		/* Red & Blue */
		 6 /* 110 */ 	140		255		140		255		140		255		/* Red & Green */
		 7 /* 111 */  	140		255		140		255		140		255		/* All RGB Combination */
		 8 /*1000 */  	140		255		140		255		140		255		/* Only for White (R=G=B) */
	>;	
	et6312b,current_range = <8
		/* Index		Rmin	Rmax	Gmin	Gmax	Bmin	Bmax	Comment */
		 1 /* 001 */  	0		0		0		0		7		71		/* Only Blue */
		 2 /* 010 */  	0		0 		7 		71 		0 		0		/* Only Green */
		 4 /* 100 */  	7		143		0		0		0		0		/* Only Red */
		 3 /* 011 */  	0		0 		8 		72 		1		33		/* Green & Blue */
		 5 /* 101 */  	25		143		0 		0 		5		61		/* Red & Blue */
		 6 /* 110 */ 	29		143		9 		49 		0		0		/* Red & Green */
		 7 /* 111 */  	14		100  	7		43		3		23		/* All RGB Combination */
		 8 /*1000 */  	7		71		3		51		1		35		/* Only for White (R=G=B) */
	>;	
};

